{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authorize",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/{everything}"
    },
    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "tracking",
          "Port": 80
        }
      ],
      "SwaggerKey": "tracking",
      "UpstreamPathTemplate": "/api/v1/tracking/{everything}"
    },

    {
      "DownstreamPathTemplate": "/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "vehicle",
          "Port": 80
        }
      ],
      "SwaggerKey": "vehicle",
      "UpstreamPathTemplate": "/api/v1/vehicle/{everything}"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "tracking",
      "Config": [
        {
          "Name": "Tracking API",
          "Version": "v1",
          "Url": "http://tracking/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "vehicle",
      "Config": [
        {
          "Name": "Vehicle API",
          "Version": "v1",
          "Url": "http://vehicle/swagger/v1/swagger.json"
        }
      ]
    }], 
  "GlobalConfiguration": {}
}